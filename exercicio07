def vetores():
  vetorA = []
  vetorB = []
  fatorial = 1
  for i in range(5):
    vetorA.append(int(input('Informe um número: ')))
    for n in range(vetorA[i],1,-1):
      fatorial = fatorial * n
    vetorB.append(fatorial)
    return vetorB
def main():
  v = vetores()
  print("O fatorial dos números da lista A são: ", v)
main()
